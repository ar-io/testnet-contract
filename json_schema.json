{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "ticker": { "type": "string" },
    "name": { "type": "string" },
    "version": { "type": "string" },
    "owner": { "type": "string" },
    "evolve": { "type": ["null", "string"] },
    "canEvolve": { "type": "boolean" },
    "records": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "contractTxID": { "type": "string" },
          "endTimestamp": { "type": "integer" },
          "startTimestamp": { "type": "integer" },
          "undernames": { "type": "number", "minimum": 10, "maximum": 10000 },
          "type": { "type": "string", "enum": ["lease", "permabuy"] }
        },
        "required": ["contractTxID", "startTimestamp", "undernames"],
        "additionalProperties": false
      }
    },
    "auctions": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "contractTxID": { "type": "string" },
          "endTimestamp": { "type": "integer" },
          "startTimestamp": { "type": "integer" },
          "type": { "type": "string", "enum": ["lease", "permabuy"] }
        },
        "required": ["contractTxID", "startTimestamp"],
        "additionalProperties": false
      }
    },
    "balances": {
      "type": "object"
    },
    "approvedANTSourceCodeTxs": {
      "type": "array",
      "items": { "type": "string" }
    },
    "fees": {
      "type": "object"
    }
  },
  "required": [
    "ticker",
    "name",
    "version",
    "owner",
    "evolve",
    "canEvolve",
    "records",
    "balances",
    "auctions",
    "gateways",
    "fees"
  ],
  "additionalProperties": false
}
